<div class='d-flex justify-content-center w-100'>
<div class='py-4 w-25'>
<h1 class='text-center'>login</h1>
<%= form_tag sessions_path do |form| %>
    
    <%= label_tag :username, 'Username(case sensitive)*',:class => 'form-label' %>
    <%= text_field_tag :usernme, nil, class: 'form_text', placeholder: 'write your username...',autofocus:
    <div class='actions mt-4'>
    <%= submit_tag 'Login', :class => 'form-btn'  %>
  </div>
<% end %>
</div>
</div>